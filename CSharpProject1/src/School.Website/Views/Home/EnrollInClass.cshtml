@model School.Website.Models.EnrollViewModel

@{
    ViewData["Title"] = "Enroll";
}
<h1>Enroll In Class.</h1>

<hr />
<div class="form-group">
    <label asp-for="ClassListSL" class="control-label"></label>
    <select asp-for="ClassId" class="form-control"
            asp-items="@Model.ClassListSL.ClassName">
        <option value="">-- Select a Class --</option>
    </select>
    <span asp-validation-for="ClassListSL.ClassId" class="text-danger" />
</div>
<div class="form-group">
    <input type="submit" value="Enroll" class="btn btn-primary" />
</div>
